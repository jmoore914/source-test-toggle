{
	"name": "source-test-toggle",
	"displayName": "Source/Test Toggle",
	"description": "Quickly switch between source and test files",
	"publisher": "jmoore914",
	"version": "0.0.1",
	"repository": "https://github.com/jmoore914/source-test-toggle.git",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sourceTestToggle"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.sourceTestToggle",
				"title": "Source/Test Toggle"
			}
		],
		"keybindings": {
			"command": "extension.sourceTestToggle",
			"key": "ctrl+alt+t"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"jest": "^24.9.0",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}